{
  "name": "next-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3030",
    "dev:entrypoint": "DEVELOPMENT_MODE=true npm run services:up",
    "build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start -p 3030",
    "start:entrypoint": "DEVELOPMENT_MODE=false npm run services:up",
    "test": "concurrently -n next,jest --hide next -k -s command-jest \"npm run services:up && wait-on tcp:5432\" \"jest --runInBand --verbose\"",
    "services:up": "docker compose up",
    "services:stop": "docker compose stop",
    "services:down": "docker compose down",
    "migrations:generate": "prisma migrate dev",
    "migrations:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "lint": "next lint"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "@prisma/extension-accelerate": "^1.3.0",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@tanstack/react-table": "^8.21.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^16.4.7",
    "jest": "^29.7.0",
    "lucide-react": "^0.486.0",
    "next": "15.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.2",
    "tailwind-merge": "^3.1.0",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.2.5",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@jest/globals": "^29.7.0",
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^29.5.14",
    "@types/node": "20.17.28",
    "@types/react": "19.0.12",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "prisma": "^6.5.0",
    "tailwindcss": "^4",
    "tsx": "^4.19.3",
    "typescript": "5.8.2",
    "wait-on": "^8.0.3"
  }
}
